    "react-native": "0.70.3",
    "react-native-svg": "^13.4.0",
    "react-native-svg-web": "^1.0.9",
    "react-native-vector-icons": "^9.2.0",
    "react-native-web": "^0.18.9",
    "use-debounce": "^8.0.4"
  },
  "devDependencies": {
    "@bahmutov/cypress-code-coverage": "^1.3.2",
